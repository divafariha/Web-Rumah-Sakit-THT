	<fieldset>
	<h2><center>FORMULIR PENDAFTARAN</center></h2>
	<form action=" formulir.html" method="get">
	Nama lengkap				: <br/><textarea rows="2" cols="70" name="nama"></textarea>
	<br/>
	Tempat Tanggal Lahir		: <br/><input type="number" name="tanggal" placeholder="tanggal"/>
								  <input type="text" name="bulan" placeholder="bulan"/>
								  <input type="number" name="tahun" placeholder="tahun"/>
	<br/>
	Jenis kelamin				: <br/><input type="radio" name="jeniskelamin" value="laki-laki" checked /> Laki - laki
								  <input type="radio" name="jeniskelamin" value="perempuan" checked /> Perempuan
	<br/>
	Alamat						: <br/><textarea rows="2" cols="70" name="alamat"> </textarea>
	<br/>
	Layanan						: <br/><input type="radio" name="layanan" value="poli" checked /> POLI THT
								  <input type="radio" name="layanan" value="audiogram" checked /> AUDIOGRAM
	<br/>
	Dokter						: <br/><select name="dokter">
								  <option value="1">Prof. dr. Fara Putri Cahyadi, Sp. THT-KL (K)</option>
								  <option value="2">Prof. dr. Freya Nur Azizah, Sp. THT-KL (K)</option>
								  <option value="3">Prof. dr. Zahran Putra Khaullah, Sp. THT-KL (K)</option>
								  <option value="4">Prof. dr. Aldo Pratama, Sp. THT-KL (K</option>
								  <option value="5">Prof. dr. Gracio Mahesa Nation, Sp. THT-KL (K)</option>
								  <option value="6">Prof. dr. Shania Andhira , Sp. THT-KL (K)</option>
								  <option value="7">Prof. dr. Anindhita Larasati, Sp. THT-KL (K)</option>
								  <option value="8">Prof. dr. Andreas Davi Herland, Sp. THT-KL (K)</option>
								  </select>
	<br><br>
	<input type="submit" value="DAFTAR">
	</form>
